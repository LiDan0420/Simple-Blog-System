<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.pipi.model.Blog">
        <table name="blog" schema="pipi" catalog=""/>
        <attributes>
            <id name="bid">
                <column name="bid" precision="11"/>
            </id>
            <basic name="title">
                <column name="title" length="50"/>
            </basic>
            <basic name="content">
                <column name="content"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
